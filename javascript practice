(12).multi dimenstional ary

var arry = [
    ["vinu",12,"be",7.7],
    ["ram,",12,"b.sc",9],
    ["kanu",12,"ba",11],
    ["jayesh",12,"baand",11],
];
document.write("<table border='1px',cellspacing='0'>");
for(var a=0; a<arry.length; a++){
    for( var b=0; b<arry.length; b++){
document.write(arry[a][b] +" ");
}
document.write("<br>");
}
document.write("</table>")

